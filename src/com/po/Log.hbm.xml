<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.po">

	<class name="Log" table="t_logs">
		<id name="lid" column="lid">
			<generator class="native"></generator>
		</id>
		<property name="ltime" column="ltime"  ></property>
		<property name="luid" column="luid" ></property>
		<property name="luser" column="luser"  ></property>
		<property name="lclass" column="lclass" ></property>
		<property name="lcourse" column="lcourse"  ></property>
		<property name="lcontrol" column="lcontrol"  ></property>
		<property name="lgenre" column="lgenre" ></property>
	</class>
	
</hibernate-mapping>
