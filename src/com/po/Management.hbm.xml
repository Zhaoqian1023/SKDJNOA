<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.po">

	<class name="Management" table="t_management">
		<id name="mid" column="mid">
			<generator class="native"></generator>
		</id>
		<property name="mname" column="mname" not-null="true"></property>
		<property name="passWord" column="passWord" not-null="true"></property>
	</class>
	
</hibernate-mapping>
